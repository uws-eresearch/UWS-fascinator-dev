{
    "url": "http://localhost:10002/api/convert",
    "outputPath": "${java.io.tmpdir}/ice2-output",
    "excludeRenditionExt": "txt,mp3,m4a",
    "resize": {
        "thumbnail": {
            "option": "fixedWidth",
            "ratio": "-90",
            "fixedWidth": "160",
            "enlarge": "false"
        },
        "preview": {
            "option": "fixedWidth",
            "ratio": "-90",
            "fixedWidth": "600",
            "enlarge": "false"
        }
    }
}
