<div class="admin-link-holder float-right">
    <a class="admin-security-link" rel="$id" href="#">Manage Access</a>
    <div class="admin-roles">
        #foreach($role in $item.getList("security_filter"))
            <div class="locked">$role</div>
        #end
        #foreach($user in $item.getList("security_exception"))
            <div class="owner">$user</div>
        #end
    </div>
    <a class="admin-delete-link" rel="$id" title="$page.escapeHtml($title)" href="#">Delete Object</a>
</div>
